        </property>
        <property>
            <shortName>a</shortName>
            <fullName>Address</fullName>
            <message>Address can take any values, but it should not be blank.</message>
            <regex>[^\s].*</regex>
        </property>
        <property>
            <shortName>e</shortName>
            <fullName>Email</fullName>
            <message>Person emails should be 2 alphanumeric/period strings separated by '@'</message>
            <regex>[\w\.]+@[\w\.]+</regex>
        </property>
        <property>
            <shortName>n</shortName>
            <fullName>Name</fullName>
            <message>Person names should only contain alphanumeric characters and spaces, and it should not be blank</message>
            <regex>[\p{Alnum}][\p{Alnum} ]*</regex>
        </property>
    </properties>
